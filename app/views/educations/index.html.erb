<p id="notice"><%= notice %></p>

<h1>Listing Educations</h1>

<table>
  <thead>
    <tr>
      <th>Division grade</th>
      <th>Obtaiend marks</th>
      <th>Years</th>
      <th>User</th>
      <th>Institute</th>
      <th>Degree</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.division_grade %></td>
        <td><%= education.obtaiend_marks %></td>
        <td><%= education.years %></td>
        <td><%= education.user %></td>
        <td><%= education.institute %></td>
        <td><%= education.degree %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education', new_education_path %>
